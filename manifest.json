{
  "name": "Open Selected Links",
  "version": "1.3",
  "description": "Opens multiple links in the selected content in a new window.",
  "manifest_version": 2,
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Open Selected Links"
  },
  "permissions": ["activeTab", "contextMenus"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
